{
    "manifest_version": 2,
    "name": "ResultSkipper",
    "version": "0.0.2",
    "description": "Skips result page.",
    "permissions": [
        "contextMenus",
        "tabs",
        "webNavigation"
    ],
    "background": {
        "scripts": ["js/background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://game.granbluefantasy.jp/*"],
            "js": ["js/content.js"],
            "run_at": "document_end"
        }
    ]    
}